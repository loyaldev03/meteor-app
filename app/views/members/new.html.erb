<%- model_class = @member.class -%>
<div class="page-header">
	<h1><%=t '.title', :default => t('helpers.titles.new', :model => model_class.model_name.human,
                               :default => "New #{model_class.model_name.human}") %></h1>
</div>

<%= form_for @member, :html => { :class => 'form-horizontal' } do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
<% end %>


<script>
  $(document).ready(new_member_functions());
</script>