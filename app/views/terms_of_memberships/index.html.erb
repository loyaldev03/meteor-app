<h1>Listing terms_of_memberships</h1>

<table>
  <tr>
    <th>Club</th>
    <th>Year price</th>
    <th>Trial days</th>
    <th>Mode</th>
    <th>Needs enrollment approval</th>
    <th>Enrollment price</th>
    <th>Max reactivations</th>
    <th>Grace period</th>
    <th>Bill type</th>
    <th>Deleted at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @terms_of_memberships.each do |terms_of_membership| %>
  <tr>
    <td><%= terms_of_membership.club_id %></td>
    <td><%= terms_of_membership.year_price %></td>
    <td><%= terms_of_membership.trial_days %></td>
    <td><%= terms_of_membership.mode %></td>
    <td><%= terms_of_membership.needs_enrollment_approval %></td>
    <td><%= terms_of_membership.enrollment_price %></td>
    <td><%= terms_of_membership.max_reactivations %></td>
    <td><%= terms_of_membership.grace_period %></td>
    <td><%= terms_of_membership.bill_type %></td>
    <td><%= terms_of_membership.deleted_at %></td>
    <td><%= link_to 'Show', terms_of_membership %></td>
    <td><%= link_to 'Edit', edit_terms_of_membership_path(terms_of_membership) %></td>
    <td><%= link_to 'Destroy', terms_of_membership, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

