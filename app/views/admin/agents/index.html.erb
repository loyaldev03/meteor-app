<%- model_class = Agent.new.class -%>
<h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>

<table id="agents_table" class="display" data-source="<%= admin_agents_url(format: "json") %>">
  <thead>
    <tr>
      <th><%= t('activerecord.attributes.agent.id') %></th>
      <th><%= t('activerecord.attributes.agent.email') %></th>
      <th><%= t('activerecord.attributes.agent.username') %></th>
      <th><%= t('activerecord.attributes.agent.locked_at') %></th>
      <th><%= t('activerecord.attributes.agent.created_at') %></th>
      <th><%= t('actions') %></th>
    </tr>
  </thead>
  <tbody>
  </tbody>
</table>

<%= link_to t(:new), new_admin_agent_path, :class => 'btn btn-primary' %>


<script>
  $(document).ready(agent_index_functions());
</script>
