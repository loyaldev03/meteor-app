<%- model_class = Partner.new.class -%>

<table id="partners_table" class="display" data-source="<%= admin_partners_url(format: "json") %>">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:prefix) %></th>
      <th><%= model_class.human_attribute_name(:name) %></th>
      <th><%= model_class.human_attribute_name(:contract_uri) %></th>
      <th><%= model_class.human_attribute_name(:website_url) %></th>
      <th><%= model_class.human_attribute_name(:description) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%= t(:actions) %></th>
    </tr>
  </thead>
  <tbody>
  </tbody>
</table>

<%= link_to t(:new),
            new_admin_partner_path,
            :class => 'btn btn-primary' %>


<script>
  $(document).ready(partner_index_functions());
</script>
