<%- model_class = Partner.new.class -%>

<%= render :partial => 'shared/page_header', :locals => { :title_text => t('.title'),:new_path => new_admin_partner_path} %>

<table id="partners_table" class="table table-bordered table-condensed dataTable" data-source="<%= admin_partners_url(format: "json") %>">
  <thead>
    <tr>
      <% Partner.datatable_columns.each do |column| %>
        <th><%= model_class.human_attribute_name(column) %></th>
      <% end %>
      <th><%= t(:actions) %></th>
    </tr>
  </thead>
  <tbody>
  </tbody>
</table>

<script>
  $(document).ready(partner_index_functions(<%= Partner.datatable_columns.size %>));
</script>
