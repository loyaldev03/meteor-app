<%= render :partial => "shared/member_information" %>

 <%= form_for @member_note, :as => :member_note, :url => member_notes_path(:id => @member_notes) do |f|%>
  <table class="table table-bordered">    
    <tr>
      <td> 
        <%= submit_tag t('add'), :class => 'btn btn-primary' %>
        <%= f.select :disposition_type_id, options_from_collection_for_select(@disposition_types, "id", "name") if !@disposition_types.nil? %> 
        <%= f.select :communication_type_id, options_from_collection_for_select(@communication_types, "id", "name") if !@communication_types.nil? %>
      </td>
    </tr>
    <tr>
      <td> <%= f.text_area :description, :style => 'width: 99%; height: 200px', :class => 'required' %> </td>
    </tr>
    <tr> <td> <%= link_to t(:back), :back, :class => 'btn' %> </td> </tr>
  </table>
 <% end %>
