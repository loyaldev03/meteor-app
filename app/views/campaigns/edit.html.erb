<div class='row-fluid'>
  <div class='span12'>
    <h2>Edit Marketing Campaign</h2>
    <hr />
  </div>
</div>
<%- model_class = @campaign.class -%>
<%= form_for @campaign, :as => :campaign, :url => campaign_path(:id => @campaign), :html => { :class => 'form-horizontal' } do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
<% end %>