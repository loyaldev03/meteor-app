<%- model_class = @domain.class -%>
<h1><%=t '.title', :default => model_class.model_name.human %></h1>

<p>
  <strong><%= model_class.human_attribute_name(:partner_id) %>:</strong><br>
  <%= @domain.partner_id %>
</p>
<p>
  <strong><%= model_class.human_attribute_name(:url) %>:</strong><br>
  <%= @domain.url %>
</p>
<p>
  <strong><%= model_class.human_attribute_name(:description) %>:</strong><br>
  <%= @domain.description %>
</p>
<p>
  <strong><%= model_class.human_attribute_name(:data_rights) %>:</strong><br>
  <%= @domain.data_rights %>
</p>
<p>
  <strong><%= model_class.human_attribute_name(:partner_id) %>:</strong><br>
  <%= @domain.partner_id %>
</p>
<p>
  <strong><%= model_class.human_attribute_name(:hosted) %>:</strong><br>
  <%= @domain.hosted %>
</p>
<p>
  <strong><%= model_class.human_attribute_name(:deleted_at) %>:</strong><br>
  <%= @domain.deleted_at %>
</p>

<div class="form-actions">
  <%= link_to t(:back),
              domains_path, :class => 'btn'  %>
  <%= link_to t(:edit),
              edit_domain_path(@domain), :class => 'btn' %>
  <%= link_to t(:destroy),
              domain_path(@domain),
              :method => 'delete',
              :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
              :class => 'btn btn-danger' %>
</div>
