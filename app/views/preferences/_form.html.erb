<div class='row-fluid'>
  <div class='span12'>
    <div class="control-group">
      <%= f.label :name, :class => 'control-label required_asterisk' %>
      <div class="controls">
        <%= f.text_field :name, :class => 'text_field', required: true %>
      </div>
    </div>
  </div>
</div>

<div class="form-actions">
  <%= f.submit nil, :class => 'btn btn-primary', :id => 'submit_button' %>
  <%= link_to t('.cancel', :default => t("cancel")), preference_group_preferences_path, :class => 'btn', id: 'cancel' %>
</div>